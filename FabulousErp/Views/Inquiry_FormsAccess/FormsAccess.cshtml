@{
    ViewBag.Title = "Forms Access";
    ViewBag.FormCode = "IUFA";
}
<div class="container-fluid">
    <div class="row mb-1">
        <label class="col-md-12 text-danger"> Filter Your Search By : </label>
        <label class="col-md-2 col-form-label col-form-label-sm"> User ID : </label>
        <div class="col-md-3">
            @Html.DropDownList("UserSearch", ViewBag.CodeList as SelectList, "-Choose-", new { @id = "InquiryUserID", @class = "form-control form-control-sm" })
        </div>
        <label class="col-md-2 col-form-label col-form-label-sm"> User Name : </label>
        <label class="col-md-3" id="UserName"></label>
    </div>

    <table class="table table-bordered table-striped display nowrap">
        <thead>
            <tr>
                <th> Forms Name </th>
                <th> Forms Code </th>
            </tr>
        </thead>
        <tbody id="formsaccess-append-data"></tbody>
    </table>
</div>

@*@Html.IncludeVersionedJs("/Scripts/jquery-3.3.1.js")*@
@Html.IncludeVersionedJs("/_Content/JS/Inquiry/Financial/GeneralSetting/UGFormsAccess/Inquiry_FormsAccess.js")
