
@using (Html.BeginForm())
{
    <fieldset class="container-fluid home-page">
        <!-- ******* Transactions Field ******** -->
        <div class="transactions-field">
            <div class="transactions-accordion">
                <div class="card">
                    <div class="card-header">
                        Transactions
                    </div>
                    <div class="card-header">
                        <ul class="nav nav-tabs card-header-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" role="tab" href="#transactions-factory">Factory</a>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <!-- Factory Content -->
                        <div id="transactions-factory" class="container tab-pane active">
                            <ul class="list-unstyled p-1">
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#transactions-Faccounting" data-toggle="collapse" role="link" aria-expanded="true">
                                        Accounting
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="transactions-Faccounting" data-parent=".transactions-accordion"></ul>
                                </li>
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#transactions-Ffixedasets" data-toggle="collapse" role="link" aria-expanded="true">
                                        Fixed Asets
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="transactions-Ffixedasets" data-parent=".transactions-accordion"></ul>
                                </li>
                                <li class="p-1 bg-light">
                                    <a href="#transactions-Fcheckbook" data-toggle="collapse" role="link" aria-expanded="true">
                                        CheckBook
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="transactions-Fcheckbook" data-parent=".transactions-accordion"></ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- ****** Setting Field ****** -->
        <div class="setting-field">
            <div class="setting-accordion">
                <div class="card">
                    <div class="card-header">
                        Setting
                    </div>
                    <div class="card-header">
                        <ul class="nav nav-tabs card-header-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" role="tablist" href="#setting-accounting">
                                    Accounting
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <div id="setting-company" class="tab-pane active container">
                            <div class="tab-content">
                                <div id="setting-accounting" class="tab-pane active">
                                    <!-- Links -->
                                    <ul class="nav nav-tabs" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="tab" role="tab" href="#setting-factory">Factory</a>
                                        </li>
                                    </ul>
                                    <div class="tab-content">
                                        <!-- Factory Accounting -->
                                        <div id="setting-factory" class="tab-pane active">
                                            <ul class="list-unstyled p-1">
                                                <li class="mb-1 p-1 bg-light">
                                                    <a href="#setting-Faccount" data-toggle="collapse" role="link" aria-expanded="true">
                                                        Account
                                                        <i class="fa fa-chevron-right pull-right"></i>
                                                    </a>
                                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="setting-Faccount" data-parent=".setting-accordion">
                                                        <li> <a href="/F_CreateAccount/FactoryAccount"> <i class=""></i> Factory Create Account </a></li>
                                                    </ul>
                                                </li>
                                                <li class="mb-1 p-1 bg-light">
                                                    <a href="#setting-Fanalytic" data-toggle="collapse" role="link" aria-expanded="true">
                                                        Analytic
                                                        <i class="fa fa-chevron-right pull-right"></i>
                                                    </a>
                                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="setting-Fanalytic" data-parent=".setting-accordion">
                                                        <li> <a href="/F_CreateAnalyticAccounts/FactoryAnalyticAccounts"> <i class=""></i> Factory Analytic Account </a></li>
                                                        <li> <a href="/F_AnalyticAccountDistribution/FactoryAnalyticDistribution"> <i class=""></i> Create Analytic Factory Distribution </a></li>
                                                        <li> <a href="/F_AddDistToAccount/DistToFactoryAccount"> <i class=""></i> Add Distribution Account To Factory Account </a></li>
                                                    </ul>
                                                </li>
                                                <li class="p-1 bg-light">
                                                    <a href="#setting-Fcost" data-toggle="collapse" role="link" aria-expanded="true">
                                                        Cost
                                                        <i class="fa fa-chevron-right pull-right"></i>
                                                    </a>
                                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="setting-Fcost" data-parent=".setting-accordion">
                                                        <li> <a href="/F_CreateCostCenter/FactoryCostCenter"> <i class=""></i> Factory Cost Center </a></li>
                                                        <li> <a href="/F_CreateCostCenterAccounts/FactoryCCAccounts"> <i class=""></i> Create Cost Center Account </a></li>
                                                        <li> <a href="/F_CreateMainCostCenter/FactoryMainCostCenter"> <i class=""></i> Create Main Cost Center </a></li>
                                                        <li> <a href="/F_UpdateMainCostCenter/FactoryUpdateGroupCC"> <i class=""></i> Update Main Cost Center </a></li>
                                                        <li> <a href="/F_AddCostAccountToAccount/CostAccountToFactoryAccount"> <i class=""></i> Add Cost Account To Factory Account </a></li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- ******* Inquiry Field ******** -->
        <div class="inquiry-field">
            <div class="inquiry-accordion">
                <div class="card">
                    <div class="card-header">
                        Inquiry
                    </div>
                    <div class="card-header">
                        <ul class="nav nav-tabs card-header-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" role="tab" href="#inquiry-factory">Factory</a>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <!-- Factory Content -->
                        <div id="inquiry-factory" class="container tab-pane active">
                            <ul class="list-unstyled p-1">
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#inquiry-Faccount" data-toggle="collapse" role="link" aria-expanded="true">
                                        Account
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="inquiry-Faccount" data-parent=".inquiry-accordion"></ul>
                                </li>
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#inquiry-Fanalytic" data-toggle="collapse" role="link" aria-expanded="true">
                                        Analytic
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="inquiry-Fanalytic" data-parent=".inquiry-accordion"></ul>
                                </li>
                                <li class="p-1 bg-light">
                                    <a href="#inquiry-Fcost" data-toggle="collapse" role="link" aria-expanded="true">
                                        Cost
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="inquiry-Fcost" data-parent=".inquiry-accordion"></ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- ******** Report Field ********** -->
        <div class="report-field">
            <div class="report-accordion">
                <div class="card">
                    <div class="card-header">
                        Reports
                    </div>
                    <div class="card-header">
                        <ul class="nav nav-tabs card-header-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" role="tab" href="#report-factory">Factory</a>
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <!-- Factory Content -->
                        <div id="report-factory" class="container tab-pane active">
                            <ul class="list-unstyled p-1">
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#transactions-Faccounting" data-toggle="collapse" role="link" aria-expanded="true">
                                        Accounting
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="report-Faccounting" data-parent=".report-accordion"></ul>
                                </li>
                                <li class="mb-1 p-1 bg-light">
                                    <a href="#transactions-Ffixedasets" data-toggle="collapse" role="link" aria-expanded="true">
                                        Fixed Asets
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="report-Ffixedasets" data-parent=".report-accordion"></ul>
                                </li>
                                <li class="p-1 bg-light">
                                    <a href="#transactions-Fcheckbook" data-toggle="collapse" role="link" aria-expanded="true">
                                        CheckBook
                                        <i class="fa fa-chevron-right pull-right"></i>
                                    </a>
                                    <ul class="collapse list-group list-unstyled" data-toggle="collapse" id="report-Fcheckbook" data-parent=".report-accordion"></ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
