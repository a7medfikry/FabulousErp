                    <div class="card-body p-0">
                        <div class="tab-content">
                            <!-- Company Content -->
                            <div id="inquiry-company" class="tab-pane active show fade container">
                                <!-- Links -->
                                <ul class="list-unstyled p-1">
                                    <li class="mb-1 p-1 bg-light NoTrans">
                                        @*<a  data-toggle="" role="link" aria-expanded="true">
                                            Accounting
                                            <i class="fa fa-chevron-right pull-right"></i>
                                        </a>*@
                                        <ul class=" list-group list-unstyled" data-toggle="" id="inquiry-accounting" data-parent=".inquiry-accordion">
                                            <li> <a href="/Inquiry_ICCOA/CompanyChartOfAccounts"> <i class=""></i> Company Chart Of Accounts </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/AccountDetails"> <i class=""></i> Inquiry OF Company Transaction Account Details </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/HistoricalAccountDetails"> <i class=""></i> Inquiry OF Company Transaction Historical Account Details </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/AccountSummary"> <i class=""></i> Inquiry OF Company Transaction Account Summary </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/HistoricalAccountSummary"> <i class=""></i> Inquiry OF Company Transaction Historical Account Summary </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/TransactionDetails"> <i class=""></i> Inquiry OF Company Transaction Details </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/BatchSecurity"> <i class=""></i> Inquiry OF Company Batches Security </a></li>
                                            <li> <a href="/Inquiry_CompanyJETransactionsDetails/AnalyticsRpt"> <i class=""></i> Inquiry OF Company Analytics Account </a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

