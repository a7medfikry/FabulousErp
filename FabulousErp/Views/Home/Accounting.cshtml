            <div class="report-accordion">
                <div class="card">
                    <div class="card-header">
                        Reports
                    </div>
                  
                    <div class="card-body p-0">
                        <div class="tab-content">
                            <!-- Company Content -->
                            <div id="report-company" class="tab-pane active container">
                                <!-- Links -->
                                <ul class="list-unstyled p-1">
                                    <li class="mb-1 p-1 bg-light NoTrans">
                                        @*<a  data-toggle="" role="link" aria-expanded="true">
                                            Accounting
                                            <i class="fa fa-chevron-right pull-right"></i>
                                        </a>*@
                                        <ul class=" list-group list-unstyled" data-toggle="" id="report-accounting" data-parent=".report-accordion">
                                            <li> <a href="/C_Reports/TrailBalance"> <i class=""></i> Trail Balance </a></li>
                                            <li> <a href="/C_Reports/AccountDetails"> <i class=""></i> Account Details </a></li>
                                            <li> <a href="/BPC/BalanceSheetReport?RptType=BallanceSheet"> <i class=""></i> Balance Sheet Report </a></li>
