@model IEnumerable<FabulousErp.Payable.Models.Payable_shipping_method>

@{
    ViewBag.Title = "Shipping Method";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ship_method)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th> 
        <th>
            @Html.DisplayNameFor(model => model.Inactive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ship_method)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td> 
        <td>
            @Html.DisplayFor(modelItem => item.Inactive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
