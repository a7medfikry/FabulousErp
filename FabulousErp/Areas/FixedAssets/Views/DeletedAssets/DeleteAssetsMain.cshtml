@model IEnumerable<FabulousDB.Models.Delete_assets_main>

@{
    ViewBag.Title = "DeleteAssetsMain";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DeleteAssetsMain</h2>
<section style="overflow:auto;">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Assets_class_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number_of_parts)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inactive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assets_number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assets_custom_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deleted_date)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Assets_class_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Number_of_parts)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Inactive)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Assets_number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Assets_custom_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Deleted_date)
                </td>

            </tr>
        }

    </table>

</section>
