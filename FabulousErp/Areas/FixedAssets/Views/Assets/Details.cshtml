@model FabulousDB.Models.Asset

@{
    ViewBag.Title = "Details Asset";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="MainDiv">
    <hr />
    @Html.Action("Edit",new {id = ViewBag.Id ,IsPartial=true})
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index",new { section ="Inquiry" })
</p>
<script>
    $(function () {
        $("#MainDiv").find("input").attr("disabled", "disabled");
        $("#MainDiv").find("select").attr("disabled", "disabled");
        $("#MainDiv").find("input[type='submit']").remove();
        $("#MainDiv").find("a").remove()
    })
</script>
@if (Request["IsNumber"] == "true")
{
    <script>
        $(function () {
            PrintThis("MainDiv", false, true, false, null, "Assets", true, true, null,"input{border:none !important;width:40% !important;}select{border:none !important;width:40% !important;}.form-group label{width:35% !important;}.form-group div{width:60%;background:red !important;}.form-group div input,.form-group div select{width:100%}")
        })
    </script>
}