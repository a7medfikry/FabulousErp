
@{
    ViewBag.Title = "CompareQutation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Compare Qutation</h2>

<div class="form-group">
    <label class="control-label col-md-2">Pr No</label>
    <div class="col-md-10">
        @Html.DropDownList("Pr_no", null, htmlAttributes: new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <label class="control-label col-md-2">Pr No</label>
        <div class="col-md-10">
            <span>Comparison BY</span>
            <span><input type="radio" name="Compare" value="1" class="Compare Price"> Price</span>
            <span><input type="radio" name="Compare" value="2" class="Compare PT"> Payment Term</span>
            <span><input type="radio" name="Compare" value="3" class="Compare DD"> Delivery Date </span>
        </div>
    </div>
</div>