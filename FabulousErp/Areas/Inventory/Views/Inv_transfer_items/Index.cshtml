@model IEnumerable<FabulousDB.Models.Inv_transfer_items>

@{
    ViewBag.Title = "transfer items";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.From_Store.Store_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.item.Item_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lot.Desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.To_Store.Store_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Transfer_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Transfer_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Posting_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ref)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Unite_cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.From_Store.Store_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.item.Item_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lot.Desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.To_Store.Store_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transfer_no)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transfer_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Posting_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ref)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Unite_cost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
