@model IEnumerable<FabulousDB.Models.Inv_sales_invoice>

@{
    ViewBag.Title = "Sales Invoice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Currency.CurrencyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Site.Site_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Store.Store_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Doc_type)
        </th>
    
        <th>
            @Html.DisplayNameFor(model => model.Batch_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Transaction_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Posting_date)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Customer_id)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Currency.CurrencyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Site.Site_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Store.Store_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Doc_type)
        </td>
   
        <td>
            @Html.DisplayFor(modelItem => item.Batch_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Transaction_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Posting_date)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.Customer_id)
        </td>
     
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
