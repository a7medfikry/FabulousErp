        <th>
            @Html.DisplayNameFor(model => model.Desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lot_Desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lot_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
