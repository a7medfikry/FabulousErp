@model IEnumerable<FabulousDB.Models.Inv_items_serial>

@{
    ViewBag.Title = "items serial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Item.Item_id)
